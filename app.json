{
  "name": "Sagiri Robot",
  "description": "Telegram Group Management Bot.",
  "logo": "https://telegra.ph/file/ee28ffa1fa36ac19faa41.jpg",
  "keywords": [
    "telegram",
    "modular",
    "group",
    "manager",
    "Dazai"
  ],
  "repository": "https://github.com/chaudharyji0805-bot/GOJOBOT",
  "stack": "heroku-22",

  "addons": [
    "heroku-postgresql:essential-0"
  ],

  "env": {
    "API_ID": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true
    },
    "ALLOW_EXCL": {
      "description": "Set this to True if you want ! to be a command prefix along with /.",
      "value": "True"
    },
    "CASH_API_KEY": {
      "description": "Get from alphavantage.co",
      "required": true
    },
    "DEL_CMDS": {
      "description": "Delete command messages from unauthorized users.",
      "value": "True"
    },
    "SQLALCHEMY_DATABASE_URI": {
      "description": "Leave EMPTY. Heroku will inject DATABASE_URL automatically.",
      "required": false
    },
    "ENV": {
      "required": true,
      "value": "True"
    },
    "EVENT_LOGS": {
      "required": true
    },
    "MONGO_DB_URI": {
      "required": true
    },
    "
